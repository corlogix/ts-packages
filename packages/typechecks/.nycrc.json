{
  "extends": "@istanbuljs/nyc-config-typescript",
  "exclude": [
    "**/index.ts",
    "**/*.test.ts"
  ],
  "reporter": [
    "html",
    "lcovonly",
    "clover",
    "text",
    "text-summary"
  ],
  "report-dir": "coverage",
  "check-coverage": true,
  "functions": 80,
  "lines": 80,
  "statements": 80,
  "branches": 70
}